{
  "name": "mvb-soft/mvb-yii2-temaplte",
  "description": "Advanced template for big projects",
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "yiisoft/yii2": "~2.0.47",
    "yiisoft/yii2-bootstrap5": "~2.0.4",
    "yiisoft/yii2-symfonymailer": "~3.0.0",
    "yiisoft/yii2-redis": "~2.0.19",
    "yiisoft/yii2-queue": "~2.3.5",
    "yiisoft/yii2-httpclient": "~2.0.15",
    "yiisoft/yii2-twig": "~2.4.2",
    "yiisoft/yii2-mongodb": "~3.0.0",
    "thamtech/yii2-uuid": "*",
    "nesbot/carbon": "^2.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "yiisoft/yii2-faker": "~2.0.0"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer" : true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}